{
  "version": 2,
  "builds": [
    { "src": "src/app.js", "use": "@vercel/node" },
    { "src": "src/**/*.html", "use": "@vercel/static" },
    { "src": "src/**/*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/auth/register", "dest": "src/app.js" },
    { "src": "/auth/login", "dest": "src/app.js" },
    { "src": "/(.*)", "dest": "src/app.js" }
  ]
}
