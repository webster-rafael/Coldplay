{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/auth/register", "dest": "src/app.js" },
      { "src": "/auth/login", "dest": "src/app.js" },
      { "src": "/(.*)", "dest": "src/app.js" }
    ]
  }
  